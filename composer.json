{
  "name": "flipboxfactory/saml-idp",
  "description": "SAML Identity Provider",
  "version": "0.0.4",
  "type": "craft-plugin",
  "keywords": [
    "cms",
    "craftcms",
    "saml",
    "sso",
    "idp",
    "identity provider",
    "federated authentication",
    "yii2"
  ],
  "require": {
    "flipboxfactory/saml-core": "^2.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4",
    "codeception/codeception": "^3.0",
    "yiisoft/yii2-faker": "^2.0",
    "phpunit/phpunit":"<8.2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\saml\\idp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\saml\\idp\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/saml-idp/issues?state=open",
    "source": "https://github.com/flipboxfactory/saml-idp",
    "docs":   "https://github.com/flipboxfactory/saml-idp",
    "rss":    "https://github.com/flipboxfactory/saml-idp/commits/master.atom"
  },
  "extra": {
    "name": "SAML Identity Provider",
    "version":"0.0.4",
    "schemaVersion":"0.0.1",
    "handle": "saml-idp",
    "class": "flipbox\\saml\\idp\\Saml",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/saml-idp/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/flipboxfactory/saml-idp/archive/master.zip"
  }
}
